{
  "header": {
    "blocks": ["header-layout.desktop", "header-layout.mobile"]
  },
  "header.full": {
    "blocks": ["header-layout.desktop", "header-layout.mobile"]
  },
  "header-layout.desktop": {
    "children": [

      "sticky-layout#4-desktop"
    ]
  },


  "rich-text#header-envio": {
    "props": {
      "text": "ENVÍOS GRATIS EN COMPRAS MAYORES A $899",
      "blockClass": "header-envio"
    }
  },



  "flex-layout.row#1-desktop": {
    "children": ["telemarketing"],
    "props": {
      "fullWidth": true
    }
  },

  "flex-layout.row#3-desktop": {
    "props": {
      "blockClass": "menu-link",
      "horizontalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true
    },
    "children": [
      "vtex.menu@2.x:menu#websites",
      "flex-layout.col#spacer",
      "vtex.menu@2.x:menu#institutional"
    ]
  },
  "flex-layout.col#spacer": {
    "props": {
      "width": "grow"
    }
  },
  "sticky-layout#4-desktop": {
    "props": {
      "blockClass": "sticky-header"
    },
    "children": ["rich-text#header-envio","flex-layout.row#4-desktop"]
  },
  "flex-layout.row#4-desktop": {
    "props": {
      "blockClass": "main-header",
      "horizontalAlign": "center",
      "verticalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true
    },
    "children": [
      "flex-layout.col#logo-desktop",
      "flex-layout.col#category-menu",
      "flex-layout.col#spacer",
      "flex-layout.row#header-right"
    ]
  },

  "flex-layout.row#header-right": {
    "props": {
      "colSizing": "auto",
       "blockClass": "header-right"
    },
    "children": [
      "search-bar",
      "login#acceso",
      "minicart.v2"
    ]
  },
  "flex-layout.col#logo-desktop": {
    "props": {
      "verticalAlign": "middle"
    },
    "children": ["logo#desktop"]
  },
  "flex-layout.col#category-menu": {
    "props": {
      "verticalAlign": "middle"
    },
    "children": ["vtex.menu@2.x:menu#category-menu"]
  },
  "logo#desktop": {
    "props": {
      "title": "Logo",
      "href": "/",
      "url": "/arquivos/header_logo.png"
    }
  },
  "header-layout.mobile": {
    "children": ["sticky-layout#1-mobile"]
  },
  "sticky-layout#1-mobile": {
    "children": [
      "rich-text#header-envio",
      "flex-layout.row#1-mobile",
      "search-bar"
    ]
  },
  "flex-layout.row#1-mobile": {
    "children": [
      "drawer",
      "logo#mobile",
      "flex-layout.col#spacer",
      "login#acceso",
      "minicart.v2"
    ],
    "props": {
      "blockClass": "main-header-mobile",
      "preventHorizontalStretch": true,
      "preserveLayoutOnMobile": true,
      "fullWidth": true
    }
  },
  "drawer": {
    "children": ["menu#drawer"]
  },

  "login#acceso": {
    "props": {
      "labelClasses": "black",
      "blockClass": "loginItem",
      "optionsTitle": "",
      "showIconProfile": true,
      "hideIconLabel": false,
      "acessCodePlaceholder": "Registro",
      "emailAndPasswordTitle": "Acceso a tu Cuenta",
      "accessCodeTitle": "Enviaremos el código de acceso a tu correo",
      "emailPlaceholder": "Correo Electrónico",
      "passwordPlaceholder": "Contraseña",
      "showPasswordVerificationIntoTooltip": true
    }
  },

  "menu#drawer": {
    "children": [
      "disclosure-layout#menu-cats",
      "menu-item#header-productos",
      "menu-item#header-acerca"
    ],
    "props": {
      "orientation": "vertical"
    }
  },


    "disclosure-layout#menu-cats": {
      "children": [
        "disclosure-trigger#menu-cats",
        "disclosure-content#menu-cats"
      ],
      "props": {
        "blockClass" : "menu-cats"
      }
    },
    "disclosure-trigger#menu-cats": {
      "children": [
        "rich-text#menu-cats"
      ]
    },
    "disclosure-content#menu-cats": {
      "children": [
        "link#menu-c1",
        "link#menu-c2",
        "link#menu-c3",
        "link#menu-c4",
        "link#menu-c5",
        "link#menu-c6",
        "link#menu-c7",
        "link#menu-c8",
        "link#menu-c9"
        /*,"link#menu-c10"*/
      ],
      "props": {
        "blockClass": "menu-cats"
      }
    },
    "rich-text#menu-cats": {
      "props": {
        "text": "Categorías",
        "blockClass":"menu-cats"
      }
    },
